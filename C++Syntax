#include<iostream>

using namespace std;

int main() {
    int a,b,c,d;
    
    // 用空格隔开 and  输入共四行，第一行包含整数 A，第二行包含整数 B，第三行包含整数 C，第四行包含整数 D。
    cin >> a >> b >> c >> d; 
    //scanf("%d%d%d%d", &a, &b, &c, &d);//
    cout << "DIFERENCA = " << (a * b) - (c * d) << endl;
}


#include <cstdio>

int main()
{
    int a, b, c, d;
    scanf("%d%d%d%d", &a, &b, &c, &d);
    printf("DIFERENCA = %d\n", a * b - c * d);
    return 0;
}

#include <cstdio>

using namespace std;

int main()
{
    double pi = 3.14159, r;
    scanf("%lf", &r);
    printf("A=%.4lf\n", pi * r * r);

    return 0;
}

#include <bits/stdc++.h>
using namespace std;
int main()
{
    double a, b, c;
    cin >> a >> b >> c;
    printf("NUMBER = %.0lf\nSALARY = U$ %.2lf",a,b*c);
    return 0;
}
